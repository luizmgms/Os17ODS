<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".DocumentsActivity"
    tools:showIn="@layout/layout_cupula">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/title_text_cup"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:layout_margin="@dimen/margin_of_texts"
            android:textSize="16sp"
            android:text="@string/text_subtitle_cup"
            tools:targetApi="O"/>

        <ImageView
            android:id="@+id/videoObjGlobId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/video_obj_glob"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginLeft="16dp"
            android:textSize="18sp"
            android:text="@string/text_title_new_agend_cup"
            android:textStyle="bold"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:layout_margin="@dimen/margin_of_texts"
            android:textSize="16sp"
            android:text="@string/text_new_agend_cup"
            tools:targetApi="O"/>

        <ImageView
            android:id="@+id/videoQeDesSusId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/video_o_que_ods"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:layout_margin="@dimen/margin_of_texts"
            android:textSize="16sp"
            android:text="@string/text_new_agend_cup2"
            tools:targetApi="O"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/title_text_process_inter"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:layout_margin="@dimen/margin_of_texts"
            android:textSize="16sp"
            android:text="@string/text_process_inter"
            tools:targetApi="O"/>

        <ImageView
            android:id="@+id/videoTransId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/video_trans_obj_milenio"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:justificationMode="inter_word"
            android:layout_margin="@dimen/margin_of_texts"
            android:textSize="16sp"
            android:text="@string/text_process_inter2"
            tools:targetApi="O"/>

        <ImageView
            android:id="@+id/videoQualoMId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/video_qual_m_dia"/>

        <TextView
            android:id="@+id/textLinkAcessAgendId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/margin_of_texts"
            android:textSize="16sp"
            android:text="@string/text_link_acess_agend"
            android:textColor="@color/colorPrimary"/>

    </LinearLayout>

</androidx.core.widget.NestedScrollView>